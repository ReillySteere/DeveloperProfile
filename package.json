{
  "name": "developer-profile",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "npm run build:server && npm run build:ui",
    "build:ui": "webpack --config webpack.browser.js --mode production",
    "build:server": "nest build && tsc-alias",
    "test": "jest --coverage",
    "start": "concurrently \"npm run start:server:dev \" \"npm run start:ui \"",
    "start:ui": "webpack serve --config webpack.browser.js --mode development",
    "start:server:debug": "nest start --debug --watch",
    "start:server:dev": "cross-env NODE_ENV=development nest start --entryFile=server/main --watch",
    "start:server:prod": "node dist/src/server/main",
    "test:server": "jest --config jest.node.ts --coverage",
    "test:ui": "jest --config jest.browser.ts --coverage",
    "scaffold": "node .github/skills/feature-scaffold/scaffold-feature.js",
    "generate-routes": "tsr generate",
    "type-check": "tsc --noEmit",
    "depcruise:verify": "npm run depcruise:server && npm run depcruise:ui",
    "depcruise:server": "depcruise src/server --config .dependency-cruiser.server.js",
    "depcruise:ui": "depcruise src/ui --config .dependency-cruiser.ui.js",
    "depcruise:graph:server": "depcruise src/server --config .dependency-cruiser.server.js --output-type dot | node scripts/dot-to-svg.js > dependency-graph-server.svg",
    "depcruise:graph:ui": "depcruise src/ui --config .dependency-cruiser.ui.js --output-type dot | node scripts/dot-to-svg.js > dependency-graph-ui.svg",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "npm run format:server && npm run format:ui && npm run format:root",
    "check-format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"*.{js,mjs,ts,json,md}\" \".github/**/*.{md,yml,yaml}\" \".vscode/*.json\" \"architecture/**/*.md\" \"scripts/**/*.{js,ts}\"",
    "format:root": "prettier --write \"*.{js,mjs,ts,json,md}\" \".github/**/*.{md,yml,yaml}\" \".vscode/*.json\" \"architecture/**/*.md\" \"scripts/**/*.{js,ts}\"",
    "format:server": "prettier --write \"src/{server,shared}/**/*.{ts,json,md}\"",
    "format:ui": "prettier --write \"src/ui/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky || true"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@tanstack/eslint-plugin-router": "^1.121.21",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@tanstack/react-router-devtools": "^1.122.0",
    "@tanstack/router-cli": "^1.145.2",
    "@tanstack/router-plugin": "^1.122.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-transition-group": "^4.4.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-ebf51a3-20250411",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react-compiler": "^19.0.0-beta-ebf51a3-20250411",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^16.2.7",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "source-map-support": "^0.5.21",
    "style-loader": "^4.0.0",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/typeorm": "^11.0.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.11.0",
    "@sentry/react": "^9.10.1",
    "@sentry/tracing": "^7.120.3",
    "@sentry/webpack-plugin": "^4.6.2",
    "@tanstack/react-query": "^5.71.0",
    "@tanstack/react-router": "^1.122.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "copy-webpack-plugin": "^13.0.1",
    "framer-motion": "^12.19.2",
    "html-webpack-plugin": "^5.6.5",
    "lucide-react": "^0.525.0",
    "mermaid": "^11.12.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-transition-group": "^4.4.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.22",
    "zustand": "^5.0.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    ".github/**/*.{md,yml,yaml}": [
      "prettier --write"
    ],
    "architecture/**/*.md": [
      "prettier --write"
    ]
  }
}
